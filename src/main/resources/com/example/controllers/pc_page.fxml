<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="330.0" prefWidth="520.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.controllers.PcPageController">
   <children>
      <Label fx:id="textLogin" layoutX="18.0" layoutY="27.0" text="Логин" />
      <Button fx:id="buttonBuy" layoutX="18.0" layoutY="60.0" mnemonicParsing="false" onAction="#buttonBuyClick" prefHeight="54.0" prefWidth="487.0" text="Купить билет" />
      <Label layoutX="18.0" layoutY="134.0" text="Ближайшие поездки" />
      <ScrollPane fx:id="scrollFlights" layoutX="18.0" layoutY="165.0" prefHeight="105.0" prefWidth="487.0">
         <content>
            <VBox fx:id="vBoxInScroll" prefHeight="200.0" prefWidth="471.0" />
         </content>
      </ScrollPane>
      <Button fx:id="buttonExit" layoutX="453.0" layoutY="280.0" mnemonicParsing="false" onAction="#buttonExitClick" text="Выйти" />
   </children>
</AnchorPane>
